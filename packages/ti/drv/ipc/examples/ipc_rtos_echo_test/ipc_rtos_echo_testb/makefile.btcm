#
ifeq ($(RULES_MAKE), )
include $(PDK_INSTALL_PATH)/ti/build/Rules.make
else
include $(RULES_MAKE)
endif

APP_NAME = ipc_rtos_echo_testb_$(BUILD_OS_TYPE)

SRCDIR = ../ ../../common/src ../../echo_test/echo_test_btcm
