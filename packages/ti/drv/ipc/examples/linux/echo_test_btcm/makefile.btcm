#
# This file is the makefile for building IPC echo testb for Linux
#
ifeq ($(RULES_MAKE), )
include $(PDK_INSTALL_PATH)/ti/build/Rules.make
else
include $(RULES_MAKE)
endif

APP_NAME = ipc_echo_testb_$(BUILD_OS_TYPE)

#Path for including common files
IPC_COMMON_PATH = ../../common

SRCDIR = . ../ $(IPC_COMMON_PATH)/src
INCDIR = $(IPC_COMMON_PATH)/$(SOC)/$(BUILD_OS_TYPE)/
SRCS_COMMON = ipc_echo_test.c

ECHO_TEST_BTCM = 1
CFLAGS_LOCAL_COMMON = -DA72_LINUX_OS  -DECHO_TEST_BTCM -DENABLE_UART_PRINT #-DA72_LINUX_OS_IPC_ATTACH

include $(IPC_COMMON_PATH)/makefile.mk

